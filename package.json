{
  "name": "flowless-api",
  "version": "1.0.0",
  "description": "Express.js API with Supabase integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"esw --watch --color --ext .ts ./src\" \"ts-node-dev --respawn --transpile-only src/index.ts\"",
    "migrate": "supabase db migrate",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage=false",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --max-warnings=0"
    ],
    "*.{js,ts}": [
      "npm run test:ci"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "eslint-watch": "^8.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "supabase": "^1.127.3",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
